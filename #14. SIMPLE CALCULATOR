#14.develop simple calculator
num1= int(input("enter your first number"))
num2= int(input("enter your second number"))
operator=input("enter the operator(+,-,*,/):")
if operator=='+':
    print("result:",num1+num2)
elif operator=='-':
    print("result:",num1-num2)
elif operator=='*':
    print("result:",num1*num2)
elif operator=='/':
    print("result:",num1/num2)
else:
    print("error")
