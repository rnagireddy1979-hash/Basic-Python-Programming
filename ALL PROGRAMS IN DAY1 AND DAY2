    print("even number")
else:
    print("odd number")

#21.divisible by 5 and 11
num=int(input("enter a number:"))
if(num % 5 == 0 and num % 11 == 0):
    print("divisible by 5 and 11")
else:
    print("not divisible by 5 and 11")

#22.check the age group given by user
age=int(input("enter the age"))
if age<13:
    print("child")
elif(age>=13 and age<=20):
    print("teenager")
else:
    print("adult")

#23.check the garde using marks
marks=int(input("enter the marks"))
if marks>=90:
    print("your garde is 'a' \n excellent")
elif(marks>=70 and marks<=80 ):
    print("your grade is 'b' \n good ")
elif(marks>=50 and marks<=70 ):
    print("your grade is 'c' \n average ")
elif(marks>=40 and marks<=50 ):
    print("your grade is 'd' \n improve yourself ")
else:
    print("you failed \n work hard ")

#24.print week of day
choice=int(input("enter your choice"))
if choice==1:
    print("sunday")
elif choice==2:
    print("monday")
elif choice==3:
    print("tuesday")
elif choice==4:
    print("wednesday")
elif choice==5:
    print("thursday")
elif choice==6:
    print("friday")
else:
    print("saturday")

