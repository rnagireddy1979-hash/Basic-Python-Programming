if(theory>+35 and prac>+15) or ((theory+prac)>60):
    print("passed")
else:
    print("failed")

#19.positive,negative or zero number
a=float(input("enter the number"))
if(a>0):
  print("positive number")
elif(a<0):
   print("negative number")
else:
    print("zero")

#20.even or odd number
a=int(input("enter a number:"))
